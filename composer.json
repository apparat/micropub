{
  "name": "apparat/micropub",
  "description": "Apparat micropub server",
  "homepage": "https://apparat.tools",
  "minimum-stability": "RC",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net",
      "homepage": "https://jkphl.is",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "apparat/server": "dev-master",
    "dominicsayers/isemail": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "satooshi/php-coveralls": "^1.0",
    "codeclimate/php-test-reporter": "dev-master",
    "apparat/dev": "@dev",
    "apparat/object": "@dev",
    "apparat/resource": "@dev",
    "apparat/kernel": "@dev",
    "clue/graph-composer": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/dev.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/resource.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/object.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/server.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dominicsayers/isemail.git",
      "reference": "master"
    },
    {
      "type": "git",
      "url": "git@github.com:jkphl/graph-composer.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    },
    "files": [
      "src/Micropub/Module.php",
      "src/Micropub/Autorun.php"
    ]
  }
}
